{
  "scripts": {
    "manpages": "scripts/manpages",
    "prepublish": "npm run manpages",
    "test": "eslint . && node test.js",
    "readme": "scripts/readme && git commit -m '[npm run readme] Update the docs' Readme.md"
  },
  "bin": {
    "{{{ name }}}": "bin/{{{ name }}}"
  },
  "files": [
    "/**/*.js",
    "!/test.js",
    "/Readme.md",
    "/License.md",
    "/bin/",
    "/manpages/"
  ],
  "man": [
    "/manpages/elm-live.1"
  ]
}
